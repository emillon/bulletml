<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "../bulletml.dtd">

<bulletml 
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

<action label="top">
 <repeat> <times>200+200*$rank</times>
 <action>

  <fire>
   <direction>120*$rand-60</direction>
   <speed>0.5+$rand*2</speed>
   <bullet/>
  </fire>

 </action>
 </repeat>

 <wait>150</wait>
</action>

</bulletml>