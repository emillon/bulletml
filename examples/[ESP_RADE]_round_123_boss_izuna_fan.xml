				 11+$rank*6
			</param>
			<param>
				 1
			</param>
			<param>
				 30
			</param>
			<param>
				 12-$rank*4
			</param>
			<param>
				 2.5-$rank
			</param>
		</actionRef>
		<actionRef  label="fanMain">
			<param>
				 11+$rank*6
			</param>
			<param>
				 1
			</param>
			<param>
				 -20
			</param>
			<param>
				 -12+$rank*4
			</param>
			<param>
				 -2.5+$rank
			</param>
		</actionRef>
		<actionRef  label="fanMain">
			<param>
				 13+$rank*8
			</param>
			<param>
				 1
			</param>
			<param>
				 30
			</param>
			<param>
				 12-$rank*4
			</param>
			<param>
				 2.3-$rank
			</param>
		</actionRef>
		<actionRef  label="fanMain">
			<param>
				 13+$rank*8
			</param>
			<param>
				 1
			</param>
			<param>
				 -20
			</param>
			<param>
				 -12+$rank*4
			</param>
			<param>
				 -2.3+$rank
			</param>
		</actionRef>
	</action>
	<action  label="fanMain">
		<fire>
			<direction  type="aim">
				180
			</direction>
			<speed>
				 0
			</speed>
			<bullet>
				<action>
					<actionRef  label="fan">
						<param>
							 $1
						</param>
						<param>
							 $2
						</param>
						<param>
							 $3
						</param>
						<param>
							 $4
						</param>
						<param>
							 $5
						</param>
					</actionRef>
				</action>
			</bullet>
		</fire>
		<wait>
			 90
		</wait>
