<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "../bulletml.dtd">
<bulletml type="vertical" xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">
	<bullet  label="Red">
		<action/>
	</bullet>
	<bullet  label="Dummy">
		<action>
			<vanish/>
		</action>
	</bullet>
	<action  label="Stop">
		<changeSpeed>
			<speed>
				 0
			</speed>
			<term>
				 1
			</term>
		</changeSpeed>
	</action>
	<action  label="XWay">
		<actionRef  label="XWayFan">
			<param>
				 $1
			</param>
			<param>
				 $2
			</param>
			<param>
				 0
			</param>
		</actionRef>
	</action>
	<action  label="XWayFan">
		<repeat>
			<times>
				 $1-1
			</times>
			<action>
				<fire>
					<direction  type="sequence">
						$2
					</direction>
					<speed  type="sequence">
						$3
					</speed>
					<bullet/>
				</fire>
			</action>
		</repeat>
	</action>
	<action  label="fan">
		<wait>
			 2
