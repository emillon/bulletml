<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "../bulletml.dtd">
<bulletml type="vertical" xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">
	<bullet  label="Dummy">
		<action>
			<vanish/>
		</action>
	</bullet>
	<action  label="XWay">
		<actionRef  label="XWayFan">
			<param>
				 $1
			</param>
			<param>
				 $2
			</param>
			<param>
				 0
			</param>
		</actionRef>
	</action>
	<action  label="XWayFan">
		<repeat>
			<times>
				 $1-1
			</times>
			<action>
				<fire>
					<direction  type="sequence">
						$2
					</direction>
					<speed  type="sequence">
						$3
					</speed>
					<bullet/>
				</fire>
			</action>
		</repeat>
	</action>
	<action  label="spiral">
		<fire>
			<speed>
				 0
			</speed>
			<bullet>
				<action>
					<repeat>
						<times>
							 30+$rank*45
						</times>
						<action>
							<fire>
								<direction  type="sequence">
									$1
								</direction>
								<speed>
									 1+$rank*2
								</speed>
								<bullet/>
							</fire>
							<actionRef  label="XWay">
								<param>
									 10
								</param>
								<param>
									 36
								</param>
							</actionRef>
							<wait>
								 6-$rank*3
							</wait>
						</action>
					</repeat>
					<vanish/>
				</action>
			</bullet>
		</fire>
		<wait>
			 225
		</wait>
	</action>
	<action  label="top1">
		<actionRef  label="spiral">
			<param>
				 7
			</param>
		</actionRef>
		<actionRef  label="spiral">
			<param>
				 -7
			</param>
		</actionRef>
	</action>
	<action  label="fan4">
		<fire>
			<direction  type="absolute">
				-$1
			</direction>
			<bulletRef  label="Dummy"/>
		</fire>
		<repeat>
			<times>
				 60+$rank*90
			</times>
			<action>
				<fire>
					<direction  type="sequence">
						$1
					</direction>
					<speed>
						 1+$rank*2
					</speed>
					<bullet/>
				</fire>
				<actionRef  label="XWay">
					<param>
						 4
					</param>
					<param>
						 90
					</param>
				</actionRef>
				<wait>
					 6-$rank*3
				</wait>
			</action>
		</repeat>
	</action>
	<action  label="top2">
		<actionRef  label="fan4">
			<param>
				 4
			</param>
		</actionRef>
	</action>
	<action  label="top3">
		<actionRef  label="fan4">
			<param>
				 -4
			</param>
		</actionRef>
	</action>
</bulletml>
