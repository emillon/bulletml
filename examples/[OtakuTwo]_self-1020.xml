<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "../bulletml.dtd">
<bulletml type="vertical" xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

<action label="top">
 <repeat><times>50</times>
  <action>  
   <fire>
    <direction type="sequence">30+1.1*5</direction>
    <speed>2+$rank*$rank*2</speed>
    <bullet/>
   </fire>
     <repeat><times>11</times>
      <action>
       <fire>
        <direction type="sequence">30</direction>
        <speed>2+$rank*$rank*2</speed>
        <bullet/>
       </fire>
      </action>
     </repeat>
   <repeat><times>4+$rank*10</times>
    <action>
     <wait>1</wait>
     <fire>
      <direction type="sequence">31.1</direction>
      <speed>2+$rank*$rank*2</speed>
      <bullet/>
     </fire>
     <repeat><times>11</times>
      <action>
       <fire>
        <direction type="sequence">30</direction>
        <speed>2+$rank*$rank*2</speed>
        <bullet/>
       </fire>
      </action>
     </repeat>
    </action>
   </repeat>
   <wait>10</wait>
  </action>
 </repeat>
</action>

<action label="top2">
 <repeat><times>50</times>
  <action>
   <fire>
    <direction type="sequence">-30-0.7*5</direction>
    <speed>2+$rank*$rank*2</speed>
    <bullet/>
   </fire>
     <repeat><times>11</times>
      <action>
       <fire>
        <direction type="sequence">-30</direction>
        <speed>2+$rank*$rank*2</speed>
        <bullet/>
       </fire>
      </action>
     </repeat>
   <repeat><times>4+$rank*10</times>
    <action>
     <wait>1</wait>
     <fire>
      <direction type="sequence">-30.7</direction>
      <speed>2+$rank*$rank*2</speed>
      <bullet/>
     </fire>
     <repeat><times>11</times>
      <action>
       <fire>
        <direction type="sequence">-30</direction>
        <speed>2+$rank*$rank*2</speed>
        <bullet/>
       </fire>
      </action>
     </repeat>
    </action>
   </repeat>
   <wait>10</wait>
  </action>
 </repeat>
</action>

</bulletml>