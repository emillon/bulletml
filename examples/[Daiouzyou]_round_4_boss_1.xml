									<fire>
										<direction  type="sequence">
											3
										</direction>
										<speed>
											 1.5
										</speed>
										<bullet/>
									</fire>
								</action>
							</repeat>
						</action>
					</repeat>
				</action>
			</repeat>
			<vanish/>
