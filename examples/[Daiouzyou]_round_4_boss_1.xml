				<fire>
					<direction  type="sequence">
						15
					</direction>
					<speed>
						 1.3
					</speed>
					<bullet>
						<action/>
					</bullet>
				</fire>
				<repeat>
					<times>
						 4
					</times>
					<action>
						<fire>
							<direction  type="sequence">
								0
							</direction>
							<speed  type="sequence">
								0.1
							</speed>
							<bullet>
								<action/>
							</bullet>
						</fire>
					</action>
				</repeat>
			</action>
		</repeat>
	</action>
	<action  label="top">
		<fire>
			<direction  type="absolute">
				110
			</direction>
			<bulletRef  label="seed"/>
		</fire>
		<fire>
			<direction  type="absolute">
				-110
			</direction>
			<bulletRef  label="seed"/>
		</fire>
		<wait>
			 400
		</wait>
