<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "../bulletml.dtd">
<bulletml type="vertical" xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">
	<action  label="Stop">
		<changeSpeed>
			<speed>
				 0
			</speed>
			<term>
				 1
			</term>
		</changeSpeed>
	</action>
	<bullet  label="Dummy">
		<action>
			<vanish/>
		</action>
	</bullet>
	<bullet  label="seed">
		<speed>
			 4
		</speed>
		<action>
			<wait>
				 10
			</wait>
			<actionRef  label="Stop"/>
			<repeat>
				<times>
					 20
				</times>
				<action>
					<wait>
						 20
					</wait>
					<repeat>
						<times>
							 3
						</times>
						<action>
							<fire>
								<direction  type="sequence">
									116+$rand*6-$rank*15
								</direction>
								<speed>
									 1.5
								</speed>
								<bullet/>
							</fire>
							<repeat>
								<times>
									 3.5+$rank*5
								</times>
