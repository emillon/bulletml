<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "../bulletml.dtd">
<bulletml type="vertical" xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">
	<bullet  label="Dummy">
		<action>
			<vanish/>
		</action>
	</bullet>
	<action  label="XWay">
		<actionRef  label="XWayFan">
			<param>
				 $1
			</param>
			<param>
				 $2
			</param>
			<param>
				 0
			</param>
		</actionRef>
	</action>
	<action  label="XWayFan">
		<repeat>
			<times>
				 $1-1
			</times>
			<action>
				<fire>
					<direction  type="sequence">
						$2
					</direction>
					<speed  type="sequence">
						$3
					</speed>
					<bullet/>
				</fire>
			</action>
		</repeat>
	</action>
	<bullet  label="curve">
		<action>
			<repeat>
				<times>
					 9999
				</times>
				<action>
					<changeDirection>
						<direction  type="relative">
							-$1*(4+$rank*$rank*4)
						</direction>
						<term>
							 10
						</term>
					</changeDirection>
					<wait>
						 10
					</wait>
				</action>
			</repeat>
		</action>
	</bullet>
	<action  label="spiral">
		<repeat>
			<times>
				 10+$rank*15
			</times>
			<action>
				<repeat>
					<times>
						 2
					</times>
					<action>
						<fire>
							<direction  type="sequence">
								$1
							</direction>
							<speed>
								 1.5+$rank*$rank*1.5
							</speed>
							<bulletRef  label="curve">
								<param>
									 $1
								</param>
							</bulletRef>
						</fire>
						<repeat>
							<times>
								 10+$rank*10-1
							</times>
							<action>
								<fire>
									<direction  type="sequence">
										36/($rank+1)
									</direction>
									<speed  type="sequence">
										0
									</speed>
									<bulletRef  label="curve">
										<param>
											 $1
										</param>
									</bulletRef>
								</fire>
							</action>
						</repeat>
						<wait>
							 6-$rank*3
						</wait>
					</action>
				</repeat>
				<wait>
					 6-$rank*3
				</wait>
				<fire>
					<direction  type="sequence">
						$1
					</direction>
					<speed>
						 1+$rank*2
					</speed>
					<bulletRef  label="Dummy"/>
				</fire>
			</action>
		</repeat>
	</action>
	<action  label="top1">
		<actionRef  label="spiral">
			<param>
				 -2
			</param>
		</actionRef>
	</action>
	<action  label="fan4">
		<fire>
			<direction  type="absolute">
				-$1
			</direction>
			<bulletRef  label="Dummy"/>
		</fire>
		<repeat>
			<times>
				 30+$rank*45
			</times>
			<action>
				<fire>
					<direction  type="sequence">
						$1
					</direction>
					<speed>
						 1+$rank*$rank*2
					</speed>
					<bullet/>
				</fire>
				<actionRef  label="XWay">
					<param>
						 4
					</param>
					<param>
						 90
					</param>
				</actionRef>
				<wait>
					 6-$rank*3
				</wait>
			</action>
		</repeat>
	</action>
	<action  label="top2">
		<actionRef  label="fan4">
			<param>
				 4
			</param>
		</actionRef>
	</action>
	<action  label="top3">
		<actionRef  label="fan4">
			<param>
				 -4
			</param>
		</actionRef>
	</action>
</bulletml>
